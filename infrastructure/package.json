{
  "name": "snapmagic-infrastructure",
  "version": "1.0.0",
  "description": "SnapMagic AWS CDK Infrastructure - Current Best Practices",
  "main": "lib/index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --require-approval never",
    "deploy:dev": "cdk deploy -c environment=dev --require-approval never",
    "deploy:staging": "cdk deploy -c environment=staging --require-approval never",
    "deploy:prod": "cdk deploy -c environment=prod --require-approval never",
    "destroy": "cdk destroy",
    "destroy:dev": "cdk destroy -c environment=dev",
    "destroy:staging": "cdk destroy -c environment=staging",
    "destroy:prod": "cdk destroy -c environment=prod",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "setup": "npm install && npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.170.0",
    "constructs": "^10.4.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "infrastructure",
    "snapmagic",
    "amplify"
  ],
  "author": "SnapMagic Team",
  "license": "MIT"
}
